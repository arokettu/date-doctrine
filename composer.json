{
    "name": "arokettu/date-doctrine",
    "description": "Doctrine support for arokettu/date",
    "keywords": ["date", "doctrine"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://sandfox.dev/php/date/doctrine.html",
    "authors": [{
        "name": "Anton Smirnov",
        "email": "sandfox+composer@sandfox.me",
        "role": "developer",
        "homepage": "https://sandfox.me/"
    }],
    "support": {
        "issues": "https://gitlab.com/sandfox/date-doctrine/-/issues",
        "source": "https://gitlab.com/sandfox/date-doctrine",
        "docs": "https://php-date.readthedocs.io/en/latest/doctrine.html",
        "chat": "https://gitter.im/arokettu/community"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\Date\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\Date\\Doctrine\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "arokettu/date": "^1.0",
        "doctrine/dbal": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.11",
        "sandfox.dev/code-standard": "^1.2023.12.09",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.22.2"
    }
}
