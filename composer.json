{
    "name": "arokettu/date-doctrine",
    "description": "Doctrine support for arokettu/date",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Smirnov",
            "email": "sandfox+git@sandfox.me"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\Date\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\Date\\Doctrine\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "arokettu/date": "^1.0",
        "doctrine/dbal": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.11",
        "sandfox.dev/code-standard": "^1.2023.12.09",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.22.2"
    }
}
